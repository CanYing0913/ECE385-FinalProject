module  ZombieRAM
(
		input [18:0] read_address,
		input Clk,

		output logic [4:0] curZomIndex
);

// mem has width of 5 bits and a total of 307200 addresses
logic [4:0] mem [0:307199];

initial
begin
	 $readmemh("zombie.txt", mem);
end


always_ff @ (posedge Clk) begin
	curZomIndex	<= mem[read_address];
end

endmodule
